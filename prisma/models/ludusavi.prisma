model LudusaviEntry {
    name    String @id
    steamId String?

    entries LudusaviPlatformEntry[]
}

model LudusaviPlatformEntry {
    ludusaviEntryName String
    ludusaviEntry     LudusaviEntry @relation(fields: [ludusaviEntryName], references: [name])

    platform Platform

    files    String[]
    registry String[]

    @@id([ludusaviEntryName, platform])
}
